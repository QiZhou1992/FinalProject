<h1>Register New User</h1>

<%= form_tag("/users", method: "get") do %>
<%= label_tag("Email") %>
<%= text_field :user,:Email %>
<br>
<%= label_tag("Password") %>
<%= text_field :user,:Password %>
<br>
<%= text_field :user,:Name %>
<%= text_field :user,:FollowCount %>
<%= submit_tag("Register") %>
<% end %>
<%= link_to 'Back', users_path %>